{
  "compilerOptions": {
    "lib": [
      "dom"
    ],
    "target": "ES5",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ES2015",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "declaration": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules"
  ]
}
